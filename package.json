{
    "name": "n8n-nodes-syncromsp",
    "version": "0.1.0",
    "description": "SyncroMSP node for n8n.",
    "license": "SEE LICENSE IN LICENSE.md",
    "homepage": "https://github.com/maelstrom96/n8n-nodes-syncromsp",
    "author": {
        "name": "alexandre-Jacques St-Jacques",
        "email": "hello@alexandre-jacques.me"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/maelstrom96/n8n-nodes-syncromsp.git"
    },
    "main": "index.js",
    "scripts": {
        "dev": "npm run watch",
        "build": "tsc && gulp",
        "tslint": "tslint -p tsconfig.json -c tslint.json",
        "watch": "tsc --watch",
        "test": "jest"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "credentials": [
            "dist/credentials/SyncroMSPApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/SyncroMSP/SyncroMSP.node.js"
        ]
    },
    "devDependencies": {
        "@types/express": "^4.17.6",
        "@types/jest": "^26.0.13",
        "@types/node": "^14.14.40",
        "@types/request-promise-native": "^1.0.17",
        "gulp": "^4.0.0",
        "jest": "^26.4.2",
        "n8n-core": "0.70.0",
        "n8n-workflow": "~0.58.0",
        "ts-jest": "^26.3.0",
        "tslint": "^6.1.2",
        "typescript": "~3.9.7"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testURL": "http://localhost/",
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "testPathIgnorePatterns": [
            "/dist/",
            "/node_modules/"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "json"
        ]
    }
}
